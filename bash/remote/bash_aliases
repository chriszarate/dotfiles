# Easier navigation: .., ..., ...., ....., ~ and -
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."

# Enable aliases to be sudoâ€™ed
alias sudo='sudo '

# Gzip-enabled `curl`
alias gurl='curl --compressed'
