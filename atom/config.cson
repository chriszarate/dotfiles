"*":
  "atom-fuzzy-grep":
    detectGitProjectAndUseGitGrep: false
    grepCommandString: "/usr/local/bin/ag -i --nocolor --nogroup --column --ignore=build --ignore=build-directory --ignore=dist --ignore=vendor --ignore=*[-.]min[-.]* --ignore=bundle.js --ignore=*.json"
  core:
    disabledPackages: [
      "whitespace"
      "php-debug"
    ]
    openEmptyEditorOnStart: false
    packagesWithKeymapsDisabled: [
      "php-debug"
    ]
    projectHome: "/Users/czarate/Documents/Scratch"
    telemetryConsent: "no"
    themes: [
      "one-dark-ui"
      "monokai"
    ]
  editor:
    fontFamily: "Fira Code"
    invisibles:
      tab: "â”ˆ"
    lineHeight: 1.4
    showInvisibles: true
    tabType: "hard"
  "exception-reporting":
    userId: "70c5597e-072a-a8ab-3d72-c1633b71a5c2"
  "fuzzy-finder":
    ignoredNames: [
      "build*"
      "dist*"
    ]
  "js-hyperclick":
    extensions: [
      ".js"
      ".jsx"
      ".json"
      ".node"
    ]
  linter: {}
  "linter-js-standard":
    checkStyleDevDependencies: true
  "linter-php":
    executablePath: "docker-php"
  "linter-shellcheck":
    enableNotice: true
  "linter-ui-default":
    panelHeight: 250
    showPanel: true
  "php-debug":
    PathMaps: []
    ServerPort: 9000
    currentPanelMode: "bottom"
    showWelcome: false
  "tree-view":
    hideVcsIgnoredFiles: true
  welcome:
    showOnStartup: false
