{
	"diagnostic-languageserver.filetypes": {
		"css": ["stylelint"],
		"javascript": ["eslint"],
		"javascriptreact": ["eslint"],
		"less": ["stylelint"],
		"php": ["phpcs"],
		"scss": ["stylelint"],
		"sh": ["shellcheck"],
		"typescript": ["eslint"],
		"typescriptreact": ["eslint"],
		"yaml": ["yamllint"]
	},
	"diagnostic-languageserver.formatFiletypes": {},
	"diagnostic-languageserver.linters": {
		"phpcs": {
			"command": "./vendor/bin/phpcs",
			"debounce": 100,
			"rootPatterns": [ "composer.json", "composer.lock", "vendor", ".git" ],
			"args": [ "--report=emacs", "-s", "-" ],
			"offsetLine": 0,
			"offsetColumn": 0,
			"sourceName": "phpcs",
			"formatLines": 1,
			"formatPattern": [
				"^.*:(\\d+):(\\d+):\\s+(.*)\\s+-\\s+(.*)(\\r|\\n)*$",
				{
					"line": 1,
					"column": 2,
					"message": 4,
					"security": 3
				}
			],
			"securities": {
				"error": "error",
				"warning": "warning"
			}
		}
	},
	"git.urlMode": "permalink",
	"intelephense.stubs": [
